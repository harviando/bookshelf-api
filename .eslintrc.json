{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": "off",
        "no-console": "off",
        "eol-last": "off",
        "no-unused-vars": "off",
        "object-curly-newline": "off",
        "arrow-parens": "off",
        "max-len": "off",
        "prefer-const": "off",
        "implicit-arrow-linebreak": "off"
    }
}
